{
  "name": "openapi-preview",
  "displayName": "OpenAPI Preview",
  "description": "Preview OpenAPI Docs",
  "publisher": "zoellner",
  "author": {
    "name": "Andreas Zoellner",
    "url": "https://github.com/zoellner"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/zoellner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zoellner/openapi-preview.git"
  },
  "bugs": "https://github.com/zoellner/openapi-preview/issues",
  "license": "MIT",
  "version": "2.1.3",
  "engines": {
    "vscode": "^1.73.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:openapiPreview.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openapiPreview.open",
        "title": "OpenAPI Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run copy-elements && npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "copy-elements": "mkdir -p ./out/node_modules/@stoplight/elements/ && cp node_modules/@stoplight/elements/{web-components.min.js,styles.min.css} ./out/node_modules/@stoplight/elements/"
  },
  "devDependencies": {
    "@stoplight/elements": "^7.7.18",
    "@types/node": "^16.18.3",
    "@types/vscode": "^1.73.1",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9"
  }
}
